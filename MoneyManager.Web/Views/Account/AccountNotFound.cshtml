@model int

@{
    ViewBag.Title = "Error 404: Not Found";
}

<div class="alert alert-danger mt-1 mb-1">
    <h4>Error 404: Not Found</h4>
    <hr/>
    <h5>
        Employee with Id @Model cannot be found
    </h5>
</div>
<a asp-action="Index" asp-controller="Account" class="btn btn-outline-primary">Go to your account overview</a>